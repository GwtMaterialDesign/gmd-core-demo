<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:m="urn:import:gwt.material.design.client.ui">

    <m:MaterialPanel>
        <m:MaterialLink text="GMD Core" activates="dp-5" textColor="WHITE" iconType="ARROW_DROP_DOWN"
                        iconPosition="RIGHT"/>
        <!-- My Dropdown -->
        <m:MaterialDropDown activator="dp-5" constrainWidth="true">
            <m:MaterialLink waves="DEFAULT" textColor="BLACK" text="GMD Core"/>
            <m:MaterialLink waves="DEFAULT" enabled="false" textColor="BLACK" text="GMD Addins" separator="true"/>
            <m:MaterialLink waves="DEFAULT" textColor="BLACK" text="GMD Table"/>
            <m:MaterialLink waves="DEFAULT" textColor="BLACK" text="GMD Charts"/>
        </m:MaterialDropDown>
    </m:MaterialPanel>
</ui:UiBinder> 
